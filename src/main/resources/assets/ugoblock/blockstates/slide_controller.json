{
  "multipart": [
    {
      "when": {
        "moving": "false", "facing": "north"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller"
      }
    },
    {
      "when": {
        "moving": "true", "facing": "north"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller_moving"
      }
    },
    
    {
      "when": {
        "moving": "false", "facing": "down"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller",
      "x": 90
      }
    },
    {
      "when": {
        "moving": "true", "facing": "down"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller_moving",
      "x": 90
      }
    },
    {
      "when": {
        "moving": "false", "facing": "east"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller",
      "y": 90
      }
    },
    {
      "when": {
        "moving": "true", "facing": "east"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller_moving",
      "y": 90
      }
    },

    {
      "when": {
        "moving": "false", "facing": "up"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller",
      "x": 270
      }
    },
    {
      "when": {
        "moving": "true", "facing": "up"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller_moving",
      "x": 270
      }
    },
   
    {
      "when": {
        "moving": "false", "facing": "south"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller",
      "y": 180
      }
    },
    {
      "when": {
        "moving": "true", "facing": "south"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller_moving",
      "y": 180
      }
    },
    
    {
      "when": {
        "moving": "false", "facing": "west"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller",
      "y": 270
      }
    },
    {
      "when": {
        "moving": "true", "facing": "west"
      },
      "apply": {
        "model": "ugoblock:block/slide_controller_moving",
      "y": 270
      }
    }
  ]
}